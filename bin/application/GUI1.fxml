<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <AnchorPane prefHeight="614.0" prefWidth="590.0">
         <children>
            <Button layoutX="240.0" layoutY="136.0" mnemonicParsing="false" onAction="#printResult" prefHeight="57.0" prefWidth="134.0" text="Print">
               <font>
                  <Font size="30.0" />
               </font></Button>
            <Label layoutX="135.0" layoutY="21.0" prefHeight="27.0" prefWidth="159.0" stylesheets="@application.css" text="Order" textAlignment="CENTER">
               <font>
                  <Font name="AppleGothic Regular" size="53.0" />
               </font></Label>
            <TextField fx:id="TableNO" layoutX="52.0" layoutY="144.0" prefHeight="48.0" prefWidth="176.0" promptText="TablNo.">
               <effect>
                  <Glow />
               </effect></TextField>
            <ComboBox fx:id="mycombo" layoutX="342.0" layoutY="60.0" onAction="#printShow" prefWidth="150.0" />
            <ListView fx:id="myListview" layoutX="22.0" layoutY="307.0" prefHeight="288.0" prefWidth="549.0" />
            <Button fx:id="myBtn2" layoutX="451.0" layoutY="211.0" mnemonicParsing="false" onAction="#printListView" prefHeight="76.0" prefWidth="106.0" text="List view" />
            <TextField fx:id="NumberofPerson" layoutX="55.0" layoutY="211.0" prefHeight="25.0" prefWidth="169.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
